{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "function",
  "description": "A defintiion of a function used in gameplay",
  "type": "object",
  "properties": {
    "parameters": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "enum": ["number", "string", "object", "array", "boolean"]
      }
    },
    "return": {
      "type": "string",
      "enum": ["number", "string", "object", "array", "boolean"]
    },
    "code": {
      "type": "string"
    }
  },
  "required": ["parameters", "return", "code"]
}